$TTL 3600
@   SOA  ns1 hostmaster.u.isucon.dev. (
    0      ; serial
    10800  ; refresh
    3600   ; retry
    604800 ; xxpire
    3600   ; ncache
)

@        60 IN NS ns1.u.isucon.dev.
@        60 IN A  <ISUCON_SUBDOMAIN_ADDRESS>
ns1      60 IN A  <ISUCON_SUBDOMAIN_ADDRESS>
pipe     60 IN A  <ISUCON_SUBDOMAIN_ADDRESS>
*        60 IN A  <ISUCON_SUBDOMAIN_ADDRESS>
